{
  "name": "lanying-im-web",
  "version": "3.0.16",
  "private": true,
  "scripts": {
    "prebuild": "yarn",
    "build": "vue-cli-service build",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve --fix",
    "prod": "vue-cli-service build --mode production --fix",
    "prepackage": "yarn prod",
    "package": "tar zcvf lanying-im-web.tar.gz dist",
    "format": "yarn prettier -w .",
    "presdk": "yarn",
    "sdk": "webpack --mode production --config ./config/webpack.sdk.config.js",
    "sdk2": "rollup --config ./config/rollup.sdk.config.js",
    "prerelease": "yarn sdk",
    "release": "./floo.sh lanying-im-web",
    "doc": "./node_modules/.bin/jsdoc -c ./jsdoc.config.json",
    "doc-md": "node_modules/jsdoc-to-markdown/bin/cli.js src/*/*/*.js src/*/*/*/*.js > doc.md"
  },
  "dependencies": {
    "core-js": "2",
    "jquery": "3.6.1",
    "vue-js-popover": "^1.2.1",
    "vuex": "^3.0.1",
    "webrtc-adapter": "^8.1.2",
    "crypto-js": "4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.16.4",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "assemblyscript": "^0.8.1",
    "axios": "^0.26.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bufferutil": "^4.0.1",
    "clean-webpack-plugin": "^1.0.1",
    "element-ui": "^2.12.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-vue": "^6.1.2",
    "highlight.js": "^11.8.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.3.7",
    "jsdoc": "^3.6.10",
    "jsdoc-to-markdown": "^7.1.1",
    "json-bigint": "1.0.0",
    "lodash": "^4.17.11",
    "long": "^4.0.0",
    "marked-highlight": "^2.0.6",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.24.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protobufjs": "^6.10.1",
    "qrcode": "^1.3.3",
    "query-string": "^6.2.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "socket.io-client": "^2.2.0",
    "strip-ansi": "^6.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "typescript": "^3.7.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "utf-8-validate": "^5.0.2",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-encoding-plugin": "^0.3.1",
    "wrapper-webpack-plugin": "^2.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "prettier"
    ],
    "rules": {
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "eslintIgnore": [
    "/src/im/",
    "/dist/",
    "/node_modules/"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
